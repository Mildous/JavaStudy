package Test;

public class BookTest {

	public static void main(String[] args) {
		
		Book b[] = new Book[3];
		
		b[0] = new Book();
		b[0].setTitle("자바의 정석");
		b[0].setWriter("남궁성");
		b[0].setCompany("도우출판");
		b[0].setYear(2016);
		
		b[0].showBook();
		
		System.out.println(b[0]);
		
		

	}

}
