package Chap01;

public class Array01 {

	public static void main(String[] args) {
		int a1 = 1, a2 = 2, a3 = 3;
		int sum=0;
		
		int[] a = new int[10];
		for(int i=0; i<a.length; i++)
			a[i] = i*10;
		
		for(int i=0; i<a.length; i++)
			System.out.println(a[i]);
			System.out.println(a.length);;
	}

}
