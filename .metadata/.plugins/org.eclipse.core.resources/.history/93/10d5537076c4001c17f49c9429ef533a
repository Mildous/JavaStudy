package Task01;
//색상, 화면크기(12, 14, 16인치), 메모리용량(4, 8, 16, 32)
public class Notebook {
	String color;
	int size;
	int memory;

	Notebook(){
		this("white", 14, 16);
	}
	
	Notebook(String color){
		this(color, 14, 16)
	}
	
	Notebook(String c, int s, int m){
		this.color = color;
		this.size = size;
		this.memory = memory;
	}
			
}
