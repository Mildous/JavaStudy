package Task01;

public class SnackTest {

	public static void main(String[] args) {
		Snack s1 = new Snack();
		s1.shape = "heart";
		s1.price = 1000;
		s1.taste = "Strawberry";
		
		Snack s2 = new Snack();
		s2.shape = "circle";
		s2.price = 1200;
		s2.taste = "Chocolate";
		
		System.out.printf("s1의 크기는 %d * %d, 모양은 %s, 가격은 %d, 맛은 %s", s1.width, s1.height, s1.shape, s1.price, s1.taste);

	}

}
