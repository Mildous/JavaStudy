package Chap04;

public class MyMath {
	int add(int a, int b) {
		return a + b;
	}
	int subtract(int a, int b) {
		return a - b;
	}
	int multiply(int a, int b) {
		return a * b;
	}
	double devide(double a, int b) {
		return a / b;
	}

	void showNum(int a, int b) {
		System.out.printf("입력한 수는 %d, %d 입니다.", a, b);
	}
}
